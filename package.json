{
  "name": "typescript-library-boilerplate",
  "version": "1.0.0",
  "description": "Typescript Minimal Starter Kit for building libraries",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack serve --config scripts/webpack.config.dev.ts --mode=development",
